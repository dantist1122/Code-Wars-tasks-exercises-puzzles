First one 8kyu:
https://www.codewars.com/kata/training-js-number-32-methods-of-math-round-ceil-and-floor/train/javascript

function roundIt(n){
  if((n+'').split('.')[0].length < (n+'').split('.')[1].length){
   return Math.ceil(n);
  }else if((n+'').split('.')[0].length>(n+'').split('.')[1].length){
   return Math.floor(n);
  }else if ((n+'').split('.')[0].length === (n+'').split('.')[1].length){
   return Math.round(n); 
   }
}

